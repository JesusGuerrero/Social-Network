<h2>Videos</h2>
<%= link_to "Add Video", :tab => params[:tab], :video => "add" %>
<div style="border:1px solid #e3e3e3">
<table>
	<tr>
		<th>Title</th>
		<th>Created</th>
		<th>Created By</th>
	</tr>
<%@network.videos.each do |v| %>
<tr>
	<td><%=v.title%></td>
	<td><%=v.created_at%></td>
<% @user = User.find( v.user_id )%>
	<td><%=@user.name%></td>
	<td><%= link_to "View", :name => @network.name,  :tab => params[:tab], :video => "show", :v_id => v.id %></td>
<%	if @user.id == current_user.id %>
	<td><%=link_to "Destroy", :controller => "videos", :action => "destroy", :id => v.id, :name => @network.name  %></td>
<% 	end %>
	</tr>
<%end%>
</table>
</div>
